    }
    if(scene === 201 && mouseX >= 230 && mouseX <= 380 && mouseY >= 230 && mouseY <= 430){
    sapOwned = true;
    scene = 200;
    println("you obtained sap!");
    } 
    if(scene === 192 && helium >= 100 && mouseX >= 150 && mouseX <= 300 && mouseY >= 30 && mouseY <= 80){
        cash += 1000000000 * cashMulti;
    }
    if(scene === 192 && lithium >= 100 && mouseX >= 150 && mouseX <= 300 && mouseY >= 90 && mouseY <= 140){
        multiplier += 100000000 * multiMulti;
    }
    if(scene === 192 && berylium >= 100 && mouseX >= 150 && mouseX <= 300 && mouseY >= 150 && mouseY <= 200){
        hydrogen += 10000000 * hydrogenMulti;
    }
    if(scene === 192 && boron >= 100 && mouseX >= 150 && mouseX <= 300 && mouseY >= 210 && mouseY <= 260){
        helium += 1000000 * heliumMulti;
    }
    if(scene === 192 && carbon >= 100 && mouseX >= 150 && mouseX <= 300 && mouseY >= 270 && mouseY <= 320){
        lithium += 100000 * lithiumMulti;
    }
    if(scene === 192 && nitrogen >= 100 && mouseX >= 150 && mouseX <= 300 && mouseY >= 330 && mouseY <= 380){
        berylium += 10000 * beryliumMulti;
    }
    if(scene === 192 && oxygen >= 100 && mouseX >= 350 && mouseX <= 500 && mouseY >= 30 && mouseY <= 80){
        boron += 1000 * boronMulti;
    }
    if(scene === 192 && flourine >= 100 && mouseX >= 350 && mouseX <= 500 && mouseY >= 90 && mouseY <= 140){
        carbon += 100 * carbonMulti;
    }
    if(scene === 192 && neon >= 100 && mouseX >= 350 && mouseX <= 500 && mouseY >= 150 && mouseY <= 200){
        nitrogen += 10 * nitrogenMulti;
    }
    if(scene === 192 && neon >= 10000 && mouseX >= 350 && mouseX <= 500 && mouseY >= 210 && mouseY <= 260){
        oxygen += 1 * oxygenMulti;
    }
};
keyTyped = function(){
    this._clearLogs();
    myString += key.toString();
    if(myString === "e"){
        save_file = [cash,multiplier,hydrogen,helium,lithium,berylium,boron,carbon,nitrogen,oxygen,flourine,neon,eventEnergy,leaf,bark,acorn,branch,tree,sap];
        println(save_file);
        myString = "";
        
    }
    if(myString === "i"){
        gameSaved = true;
        myString = "";
    }
};
