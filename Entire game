var drawStuff = function(){
    var cashE = cash.toExponential(1);
    var multiplierE = multiplier.toExponential(1);
    var hydrogenE = hydrogen.toExponential(1);
    var heliumE = helium.toExponential(1);
    var lithiumE = lithium.toExponential(1);
    var beryliumE = berylium.toExponential(1);
    var boronE = boron.toExponential(1);
    var carbonE = carbon.toExponential(1);
    var nitrogenE = nitrogen.toExponential(1);
    var oxygenE = oxygen.toExponential(1);
    var flourineE = flourine.toExponential(1);
    var neonE = neon.toExponential(1);
    fill(0, 0, 0);
    textSize(20);
    text("cash:",0,0,100,100);
    text("multiplier:",0,20,100,100);
    text("hydrogen:",0,40,100,100);
    text("helium:",0,60,100,100);
    text("lithium:",0,80,100,100);
    text("berylium:",0,100,100,100);
    text("boron:",0,120,100,100);
    text("carbon:",0,140,100,100);
    text("nitrogen:",0,160,100,100);
    text("oxygen:",0,180,100,100);
    text("flourine:",0,200,100,100);
    text("neon:",0,220,100,100);
    text(cashE,50,0,1000,1000);
    text(multiplierE,90,20,1000,1000);
    text(hydrogenE,90,40,1000,1000);
    text(heliumE,70,60,1000,1000);
    text(lithiumE,70,80,1000,1000);
    text(beryliumE,85,100,1000,1000);
    text(boronE,60,120,1000,1000);
    text(carbonE,70,140,1000,1000);
    text(nitrogenE,80,160,1000,1000);
    text(oxygenE,75,180,1000,1000);
    text(flourineE,75,200,1000,1000);
    text(neonE,50,220,1000,1000);
    fill(0, 255, 0);
    rect(0,499,599,99);
    fill(0,0,0);
    textSize(50);
    text("elements",200,530,1000,1000);
};
